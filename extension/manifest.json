{

  "manifest_version": 2,
  "name": "R1",
  "version": "1.0",

  "description": "Allows you to use the R1 extension on dataverse",

  "icons": {
    "48": "icons/kfg.png"
  },

  "content_scripts": [
    {
      "matches": ["*://dataverse.harvard.edu/dataset*"],
      "js": ["jquery.min.js", "add_buttons.js"]
    }
  ]

}