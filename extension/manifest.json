{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://dataverse.harvard.edu/dataset*"],
      "js": ["jquery.min.js", "add_buttons.js"]
    }
  ]

}